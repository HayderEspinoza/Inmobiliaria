@media (min-width: 1200px)
	.toggle
		background #ffcc29
		position fixed
		left 0
		top 50%
		z-index 89
		line-height 12px
		text-align center
		padding 37px 17px
		margin-top -65px
	.icon-menu
		font-size 30px
	.toggle-name
		font-size 15px
		color #222
	.menu
		width 550px !important
		padding 80px 60px 0 100px
		.cerrar
			left 5%
			top 50%
	.menu-hidden
		margin-left -550px
	.carousel-indicators
		left 45% !important
		bottom 30px
		li
			float left
			margin-right 30px !important
	.item
		.fill
			.container 
				height 100%
				.wrap-title
					height 100%	
					position relative
					padding-top 25%
				.link-proyectos
					height 350px
					margin-top -350px
				h2
					font-size 50px
					padding-top 50px
					vertical-align middle
					
	.busqueda-portada
		visibility visible
		position absolute
		top 70px
		left 75%
		border 1px solid white
		color white
		border-radius(2px)
		line-height 15px
		padding 6px
		cursor pointer
		&:hover
			color white
		.icono-search
			padding 6px 10px 8px 0
			border-right 1px solid white
	.formulario-portada
		background rgba(0, 0, 0, 0.25)
		width 320px
		min-height 500px
		padding 50px 20px 30px 20px
		position absolute
		top 100px
		left 75%
	.destacados
		.title-destacados
			span
				&:after, &:before
					content ''
					width 100px
					height 1px
					background #8b8b8d
					position absolute
					top 50%
					right -105px
				&:before
					left -105px
	
	